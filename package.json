{
    "name": "imageboard-api",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "devstart": "nodemon ./bin/www",
        "test": "jest",
        "detect": "jest --detectOpenHandles",
        "pretty-quick": "pretty-quick"
    },
    "dependencies": {
        "body-parser": "1.18.2",
        "cookie-parser": "1.4.3",
        "debug": "2.6.9",
        "express": "4.16.3",
        "morgan": "1.9.0",
        "pg": "7.4.3",
        "winston": "3.1.0"
    },
    "devDependencies": {
        "husky": "1.1.2",
        "jest": "23.6.0",
        "nodemon": "1.17.5",
        "prettier": "1.14.3",
        "pretty-quick": "1.8.0",
        "supertest": "3.3.0"
    },
    "jest": {
        "clearMocks": true
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && npm test",
            "pre-push": "npm test"
        }
    }
}
